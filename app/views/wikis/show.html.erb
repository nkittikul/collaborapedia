<h1><%= @wiki.name %></h1>
<p><%= @wiki.description %> </p>
<%= link_to "Edit Wiki", edit_wiki_path, class: 'btn btn-success' %>
<%= link_to "Edit Collaborators", wiki_collaborations_path(@wiki), class: 'btn btn-success' %>
<%= link_to "New Article", new_wiki_article_path(@wiki), class: 'btn btn-success' %>

<h2>Articles:</h2>
<% @articles.each do |article| %>
  <div>
    <%= link_to article.title, wiki_article_path(article.wiki, article) %>
  </div>
<% end %>